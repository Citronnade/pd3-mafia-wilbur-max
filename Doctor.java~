import java.util.*;

public class Doctor extends Player{
    public Doctor(String name){
	super();
	this.name = name;
	priority = 2;
    }

    public void act(){
	Player other;
	other = chooseTarget();
	if (false){
	    return 0;
	}
	else{
	    save(other);
	    return 0;
	}
	///NEED A SAVE ROUTINE
    }
}
